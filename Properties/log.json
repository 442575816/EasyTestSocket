{
  "logs":[
    {
      "name": "root",
      "target": "file",
      "rolling": {
        "type": "size",
        "size": "50m",
        "maxCount": 20,
        "path": "../logs/common/comm.log"
      },
      "level": "info",
      "format": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff}#[{Level:u4}]#{Message:lj}{NewLine}{Exception}",
      "console": true,
      "async": true
    }
  ],
  "mappings": {
    "root": "root"
  }
}